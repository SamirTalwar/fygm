[user]
	name = Samir Talwar
	email = samir@noodlesandwich.com
[core]
	editor = vim
	excludesfile = ~/.gitignore
	autocrlf = input
	ignorecase = false
[color]
	ui = true
[push]
	default = simple
[alias]
	a = add -A
	amend = commit --amend --no-edit
	name = rev-parse --abbrev-ref HEAD
	up = ! git checkout master && git pull -p && git branch --merged | egrep -v '^\\*' | xargs git branch -d
